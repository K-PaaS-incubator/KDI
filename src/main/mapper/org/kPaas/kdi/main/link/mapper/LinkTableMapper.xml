<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.kPaas.kdi.main.link.mapper.LinkTableMapper">

	<select id="getDetailVo" resultType="org.kPaas.kdi.main.link.vo.LinkDetailVo" parameterType="string">
		 SELECT * FROM KDI_LINK_DETAIL
		 <where>
		 svc_lnk_id = #{svc_lnk_id} and ds_nm =#{ds_nm} and svc_id = #{svc_id}
		 </where>
	</select>
	
	<select id="getDetailList" resultType="map" parameterType="String">
		 SELECT * FROM KDI_LINK_DETAIL
		 <where>
		 svc_id = #{svc_id} <!--  #{svc_id, JDBCType=INTEGER} (parameterType이 각 변수마다 다를경우 직접 선언) -->
		 </where>
	</select>

</mapper>